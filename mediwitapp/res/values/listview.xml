<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="list_item">
        <item name="android:paddingTop">5sp</item>
        <item name="android:paddingBottom">5sp</item>
        <item name="android:paddingRight">17sp</item>
        <item name="android:paddingLeft">18sp</item>
        <item name="android:textColor">#151515</item>
        <item name="android:background">@drawable/color_bg_content</item>
    </style>
    <style name="list_title" parent="@android:style/TextAppearance">
        <item name="android:textSize" >16sp</item>
        <item name="android:textColor">#151515</item>
        <item name="android:layout_marginLeft">10sp</item>
        <item name="android:layout_marginTop">2sp</item>
    </style>
    <style name="list_content" parent="@android:style/TextAppearance">
        <item name="android:textSize" >13sp</item>
        <item name="android:textColor">#989898</item>
        <item name="android:layout_marginLeft">10sp</item>
        <item name="android:layout_marginTop">5sp</item>
    </style>

    <style name="list_img">
        <item name="android:padding">10sp</item>
        <item name="android:divider">@drawable/blue</item>
        <item name="android:dividerHeight">2sp</item>
    </style>
</resources>
